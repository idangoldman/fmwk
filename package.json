{
  "name": "feature",
  "title": "Feature",
  "description": "Opinionated javascript framework, built with jQuery and Rails in mind.",
  "version": "0.0.4",
  "author": "Idan Goldman",
  "license": "MIT",
  "main": "./src/index.js",
  "homepage": "https://github.com/idangoldman/feature",
  "repository": {
    "type": "git",
    "url": "https://github.com/idangoldman/feature.git"
  },
  "bugs": {
    "url": "https://github.com/idangoldman/feature/issues"
  },
  "type": "module",
  "moduleRoots": [
    "src/"
  ],
  "scripts": {
    "test": "./node_modules/.bin/jest --config ./jest.config.json",
    "test:watch": "./node_modules/.bin/jest --config ./jest.config.json --watchAll",
    "test:coverage": "./node_modules/.bin/jest --config ./jest.config.json --coverage",
    "test:coverage:open": "open coverage/lcov-report/index.html",
    "test:update:snapshot": "./node_modules/.bin/jest --config ./jest.config.json --updateSnapshot",
    "docs": "./node_modules/.bin/jsdoc --configure jsdoc.json",
    "docs:open": "open docs/index.html",
    "docs:wiki": "DIR='docs'; mkdir $DIR; ./node_modules/.bin/jsdoc2md ./src/**/*.js --configure ./jsdoc.json > $DIR/index.md"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.0",
    "jest-runner-eslint": "^0.10.0",
    "jsdoc": "^3.6.5",
    "jsdoc-to-markdown": "^6.0.1"
  }
}
